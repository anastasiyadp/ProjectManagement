@model IEnumerable<ProjectManagement.Models.Project>

@{
    ViewBag.Title = "Проекты";
}

<a href="/Home/Create ">Создать</a>
<table>
    <tr>
        <th>Название</th>
        <th>Дата начала</th>
        <th>Дата окончания</th>
        <th></th>
    </tr>
    @foreach (ProjectManagement.Models.Project project in Model)
    {
    <tr>
        <td>
            @*@Html.DisplayFor(modelItem => item.name)*@
            @Html.ActionLink(project.Name, "OneProject", new { id = project.ProjectId })
        </td>
        <td>
            @project.StartDate.ToString("d")
        </td>
        <td>
            @project.FinishDate.ToString("d")
        </td>
        @*<td>
            @Html.ActionLink("Удалить", "Delete", new { id = project.ProjectId })
        </td>*@
    </tr>
    }
</table>